public class Fowler extends Object {

  public Fowler() {
    super();
  }

  public void printOwning(double amount) {
    Name name;
    this.printBanner();
 
  << {
    name := _name;

    --Print banner
    System_out.println("name:" + _name);
    System_out.println("amount" + amount);
  } >>
  }

}

